{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "make debug test",
      "type": "shell",
      "command": "make",
      "args": ["DEBUG=1", "test"],
      "options": {
        "cwd": "${fileDirname}/.."
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "make test",
      "type": "shell",
      "command": "make",
      "args": ["test"],
      "options": {
        "cwd": "${fileDirname}/.."
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "make clean",
      "type": "shell",
      "command": "make",
      "args": ["clean"],
      "options": {
        "cwd": "${fileDirname}/.."
      },
      "problemMatcher": ["$gcc"]
    }
  ]
}
